export LC_CTYPE="en_US.UTF-8"

urxvtd -q -o -f &

xset +fp ~/.fonts
xset +fp /usr/local/lib/X11/fonts/terminus
xset +fp /usr/local/lib/X11/fonts/anonymous-pro

xset fp rehash

feh --bg-center ~/.background.png &

while :; do
	xsetroot -name "`~/bin/dwmbar`"
	sleep 5
done &
kill $!

xset -b &
xmodmap ~/.xmodmap &
#exec awesome
#exec spectrwm
#exec xmonad
exec dwm
